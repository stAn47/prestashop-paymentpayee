{
    "type": "prestashop-module",
    "name": "prestashop/paymentpayee",
    "description": "PrestaShop module paymentpayee",
    "homepage": "https://github.com/stAn47/prestashop-paymentpayee",
    "license": "AFL-3.0",
    "authors": [
        {
            "name": "Payee.no",
            "email": "contact@payee.no"
        }
    ],
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "preferred-install": "dist",
        "classmap-authoritative": true,
        "optimize-autoloader": true,
        "sort-packages": true,
        "prepend-autoloader": false
    },
    "autoload": {
        "classmap": [
            "controllers",
            "paymentpayee.php"
        ]
    },
    "scripts": {
        "set-license-header": [
            "@php ./vendor/bin/header-stamp --license=\"assets/afl.txt\" --exclude=\".github,vendor\""
        ]
    },
    "require-dev": {
        "prestashop/php-dev-tools": "~3.0"
    },
    "author": "RuposTel.com"
}
